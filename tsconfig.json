{
	"compilerOptions": {
		"target": "es2020",
		"module": "es2020",
		"sourceMap": true,
		"rootDir": "./src",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"strictNullChecks": false,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"moduleResolution": "node",
		"importHelpers": true
	}
}